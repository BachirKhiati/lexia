# Synapse Environment Variables
# Copy this file to .env and fill in your API keys

# Claude API (for Socratic teaching and quest generation)
CLAUDE_API_KEY=your_claude_api_key_here

# Google Gemini API (for translation and grammar checking)
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI API (for Whisper speech-to-text)
OPENAI_API_KEY=your_openai_api_key_here

# Default AI provider (claude or gemini)
DEFAULT_AI_PROVIDER=claude
